#suite s21_memcpy_suite

#test s21_memcpy_test_1
char test_str1[] = "abcdefg";
char test_str2[] = "zxcvbnm";
int n = 5;
s21_memcpy(test_str1, test_str2, n);
ck_assert_int_eq(s21_memcmp(test_str1, test_str2, n), 0);

